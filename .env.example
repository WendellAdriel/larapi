BROADCAST_DRIVER=log
QUEUE_CONNECTION=redis

# CACHE CONFIG
CACHE_DRIVER=redis
CACHE_DEFAULT_TIME=3600

# SESSION CONFIG
SESSION_DRIVER=redis
